# Uncomment this line to define a global platform for your project
platform :ios, '7.0'

source 'https://github.com/CocoaPods/Specs.git'

def add_common_dependencies
    pod 'AFNetworking'
end

target 'GBuddy' do
	add_common_dependencies
end

target 'GBuddyTests' do
    add_common_dependencies
end

